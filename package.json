{
  "name": "pyptron",
  "version": "0.0.0-development",
  "description": "Librer√≠a para el pico y placa en Colombia",
  "private": true,
  "main": "src/api",
  "license": "MIT",
  "homepage": "https://www.pyphoy.com",
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "JEST_JUNIT_OUTPUT=test-results/jest/results.xml jest --ci --coverage --testResultsProcessor=jest-junit",
    "start:server": "sam local start-api --port 3246",
    "start:dev": "run-p build:watch start:server",
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "sync:assets": "aws s3 sync assets/ s3://pyphoy/docs --acl public-read",
    "sam:package": "sam package --template-file template.yaml --s3-bucket pyptron --output-template-file packaged.yaml",
    "sam:deploy": "sam deploy --template-file packaged.yaml --stack-name pyptron --capabilities CAPABILITY_IAM",
    "sam:validate": "sam validate -t template.yaml",
    "outputs": "aws cloudformation describe-stacks --stack-name pyptron --query \"Stacks[].Outputs[]\"",
    "deploy": "cross-env NODE_ENV=production run-s sam:validate build sam:package sam:deploy"
  },
  "bugs": {
    "url": "https://bitbucket.org/archemiro/pyptron/issues",
    "email": "pyptron@pyphoy.com"
  },
  "keywords": [
    "Colombia",
    "Pico y placa"
  ],
  "directories": {
    "lib": "lib"
  },
  "author": "Mauricio Robayo <archemiro@gmail.com> (https://www.archemiro.com)",
  "dependencies": {
    "dayjs": "^1.7.8",
    "pascua": "^1.1.7",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/archemiro/pyptron.git"
  }
}
